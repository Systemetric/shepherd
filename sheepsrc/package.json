{
  "name": "pysheep",
  "version": "1.0.0",
  "engines": {
    "node": "8.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "rimraf ../shepherd/blueprints/staticroutes/editor/*.* && cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "vuepress:update": "rimraf ./docs/full && rimraf ./docs/*.md && git clone --depth 1 https://github.com/systemetric/hr-robocon.org ./docs/full && cpy ./docs/full/docs/*.md ./docs/ && cpy ./docs/full/.vuepress/public/images/*.* ./docs/images/ && cpy ./docs/full/.vuepress/categories.json ./docs/.vuepress/ && cpy ./docs/full/.vuepress/public/*.pdf ./docs/.vuepress/public && cpy ./docs/full/.vuepress/public/*.zip ./docs/.vuepress/public && cpy ./docs/full/.vuepress/public/*.py ./docs/.vuepress/public && node ./docs/transform.js && rimraf ./docs/full",
    "vuepress:dev": "vuepress dev docs",
    "vuepress:build": "vuepress build docs --dest ../shepherd/blueprints/staticroutes/docs/",
    "updatebuild": "npm run vuepress:update && npm run vuepress:build && npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-pro": "^5.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/pro-regular-svg-icons": "^5.5.0",
    "@fortawesome/pro-solid-svg-icons": "^5.5.0",
    "@fortawesome/vue-fontawesome": "^0.1.2",
    "ajv": "^6.5.5",
    "fast-deep-equal": "^2.0.1",
    "file-saver": "^2.0.0-rc.4",
    "json-schema-traverse": "^0.4.1",
    "jszip": "^3.1.5",
    "lodash": "^4.17.13",
    "minireset.css": "0.0.3",
    "monaco-editor": "^0.14.3",
    "monaco-languageclient": "^0.9.0",
    "node-blockly": "^1.0.35",
    "normalize-url": "^3.3.0",
    "raw-loader": "^0.5.1",
    "reconnecting-websocket": "^4.1.10",
    "uri-js": "^4.2.2",
    "vscode-languageserver-types": "^3.13.0",
    "vscode-ws-jsonrpc": "^0.0.2-1",
    "vue": "^2.5.17",
    "vue-prism-component": "^1.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.0",
    "@types/jszip": "^3.1.4",
