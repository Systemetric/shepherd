    "@types/normalize-url": "^1.9.1",
    "@types/vue": "^2.0.0",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "front-matter": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.4",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.2",
    "terser": "^3.10.8",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.5",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.4",
